{
  "name": "orb-controls",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-react-app": "^2.0.1",
    "orb-grid": "0.0.6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-scripts": "0.8.4",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prepublish": "rimraf lib && npm run compile",
    "compile": "npm run compile:js && npm run compile:css",
    "compile:js": "NODE_ENV=production babel src/controls -d lib --presets react-app --ignore test.js",
    "compile:css": "postcss --use autoprefixer --use cssnano -o lib/index.css src/controls/index.css"
  },
  "peerDependencies": {
    "orb-grid": "^0.0.6",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-redux": "^4.4.6"
  },
  "dependencies": {
    "autoprefixer": "^6.5.4",
    "postcss-cli": "^2.6.0",
    "react-overlays": "^0.6.10",
    "react-resizable": "^1.4.5",
    "react-virtualized-checkbox": "^2.0.3"
  }
}
